{
  "manifest_version": 3,
  "name": "postToP",
  "description": "preMiD alternative",
  "version": "1.0",
  "permissions": [
    "tabs",
    "scripting",
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://music.youtube.com/*",
    "https://www.youtube.com/*"
  ],
  "background": {
    "service_worker": "js/background.js"
  }
}